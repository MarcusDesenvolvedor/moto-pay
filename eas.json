{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "production",
        "EAS_NO_VCS": "1",
        "GRADLE_OPTS": "-Dorg.gradle.daemon=false"
      },
      "cache": {
        "disabled": true
      },
      "android": {
        "withoutCredentials": true,
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
